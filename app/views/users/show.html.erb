<% content_for(:title, "See Your Profile | Share Your Photos!") %>
<h3><u>See Your Profile</u></h3>
<div class="col-md-9 row">
  <h4> Photos in which you are tagged:
    <% @tags.each do |tag| %>
      <ul>
        <li><h5><%= image_tag tag.photo.the_photo.url(:small), :class => 'pull_left' %>
                &nbsp&nbspcontributed by <%= tag.photo.user.email %></h5></li>
      </ul>
    <% end %>
</div>
<div class="col-md-9 row">
  <hr>
  <h4><%= link_to "Add a New Photo", new_photo_path %></h4>
  <h4><%= link_to "See All Photos", photos_path %></h4>
  <h4><%= link_to "Logout", session_path("current"), :method => "delete" %></h4>
</div>

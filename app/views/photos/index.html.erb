<% content_for(:title, "All Photos | Share Your Photos!") %>
<div class='container'>
  <h3><u>Show All Photos</u></h3>
    <% @photos.each do |photo| %>
      <ul>
        <li><h5><%= link_to (image_tag photo.the_photo.url(:small), :class => 'pull_left'), edit_photo_path(photo) %>&nbsp&nbspcontributed by <%= photo.user.email %></h5></li>
      </ul>
    <% end %>
  <br><br>

  <div class="col-md-4">
    <hr>
    <h4><%= link_to "Add a New Photo", new_photo_path %></h4>
    <h4><%= link_to "See Your Profile", user_path(current_user.id) %></h4>
    <h4><%= link_to "Logout", session_path("current"), :method => "delete" %></h4>
  </div>
</div>
